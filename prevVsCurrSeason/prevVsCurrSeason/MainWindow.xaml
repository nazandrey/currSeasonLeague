        <DataGrid x:Name="summonerLeagueDataGrid" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="221" Width="392" FrozenColumnCount="2" AutoGenerateColumns="False" IsSynchronizedWithCurrentItem="True" IsManipulationEnabled="True">
